+	-for file in $(srcdir)/faq/stylesheet-images/*.png $(srcdir)/faq/stylesheet-images/*.gif; do \
+	  basefile=`basename $$file`; \
+	  $(INSTALL_DATA) $$file $(DESTDIR)$(glade_helpdir)/faq/stylesheet-images/$$basefile; \
